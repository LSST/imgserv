<?xml version="1.0" encoding="UTF-8"?>
<uws:job xmlns:uws="http://www.ivoa.net/xml/UWS/v1.0">
  <uws:jobId>{{ uws_job_id }}</uws:jobId>
  <uws:runId>test</uws:runId>
  <uws:ownerId xsi:nil="true" />
  <uws:phase>{{ soda_job_phase }}</uws:phase>
  <uws:startTime/>
  <uws:endTime/>
  <uws:executionDuration>{{ soda_exec_duration }}</uws:executionDuration>
  <uws:parameters>
    <uws:parameter id="LANG">SODA</uws:parameter>
    <uws:parameter id="REQUEST">getImage</uws:parameter>
    <uws:parameter id="POS">{{ soda_pos }}</uws:parameter>
  </uws:parameters>
  <uws:results>
    <uws:result>{{ job_result_url }}</uws:result>
  </uws:results>
</uws:job>
