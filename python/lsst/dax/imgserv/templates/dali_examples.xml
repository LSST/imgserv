    <div id="bbox" resource="#bbox" typeof="example">
    <p> Example 1: <h3 property="name">Synchronous Query by BBOX </h3>
        <a href="{{ url_for('api_image_soda.img_sync', _external=True) }}">
            Query by BBOX, pixel
        </a>
        <span property="capability">ivoa://ivoa.net/std/SODA#snc-1.0</span>
        <span property="generic-parameter" typeof="keyval">
            <span property="key">ID</span>
            <span property="value">default.calexp.r</span>
            <span property="key">POS</span>
            <span property="value">BBOX 216.68 -0.53 100 100 pixel</span>
        </span>
    </p>
    </div>

